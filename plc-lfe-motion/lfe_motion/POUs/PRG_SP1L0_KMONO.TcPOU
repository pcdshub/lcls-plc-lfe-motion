Main.M50.bHardwareEnable      := TRUE;
Main.M50.bPowerSelf           := TRUE;
fbMotion2(stMotionStage:=Main.M50);
fbFFO2(stMotionStage := Main.M50,
       fbFFHWO := GVL.fbFastFaultOutput1,
